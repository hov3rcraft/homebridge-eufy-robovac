{
  "pluginAlias": "EufyRobovac",
  "pluginType": "platform",
  "headerDisplay": "Configure your RoboVac devices here.",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "devices": {
        "description": "For information on how to obtain deviceId and localKey, go [here](https://github.com/hov3rcraft/eufy-robovac#readme).",
        "title": "Devices",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "title": "Name",
              "type": "string",
              "default": "Vacuum Cleaner",
              "required": true
            },
            "model": {
              "title": "Model",
              "description": "Your device's model. Try \"default\" if your model is not listed.",
              "type": "string",
              "required": true,
              "default": "default",
              "oneOf": [
                { "title": "RoboVac 11C (T2103)", "enum": ["T2103"] },
                { "title": "RoboVac 11S Plus (T2119)", "enum": ["T2119"] },
                { "title": "RoboVac 15C (T2120)", "enum": ["T2120"] },
                { "title": "RoboVac 15C MAX (T2128)", "enum": ["T2128"] },
                { "title": "RoboVac 25C (T2123)", "enum": ["T2123"] },
                { "title": "RoboVac 25C MAX (T2132)", "enum": ["T2132"] },
                { "title": "RoboVac 30C (T2118)", "enum": ["T2118"] },
                { "title": "RoboVac 30C MAX (T2130)", "enum": ["T2130"] },
                { "title": "RoboVac 35C (T2117)", "enum": ["T2117"] },
                { "title": "RoboVac C20 Hybrid SES (T2280)", "enum": ["T2280"] },
                { "title": "RoboVac G10 Hybrid (T2150)", "enum": ["T2150"] },
                { "title": "RoboVac G30 (T2250)", "enum": ["T2250"] },
                { "title": "RoboVac G30 Edge (T2251)", "enum": ["T2251"] },
                { "title": "RoboVac G30 Hybrid (T2253)", "enum": ["T2253"] },
                { "title": "RoboVac G30 Verge (T2252)", "enum": ["T2252"] },
                { "title": "RoboVac G32 Pro (T2259)", "enum": ["T2259"] },
                { "title": "RoboVac G35 (T2254)", "enum": ["T2254"] },
                { "title": "RoboVac G35+ (T2270)", "enum": ["T2270"] },
                { "title": "RoboVac G40 (T2255)", "enum": ["T2255"] },
                { "title": "RoboVac G40 Hybrid+ (T2273)", "enum": ["T2273"] },
                { "title": "RoboVac G40+ (T2272)", "enum": ["T2272"] },
                { "title": "RoboVac L35 Hybrid (T2194)", "enum": ["T2194"] },
                { "title": "RoboVac L35 Hybrid+ (T2192)", "enum": ["T2192"] },
                { "title": "RoboVac L50 SES (T2275)", "enum": ["T2275"] },
                { "title": "RoboVac L60 (T2267)", "enum": ["T2267"] },
                { "title": "RoboVac L60 Hybrid (T2268)", "enum": ["T2268"] },
                { "title": "RoboVac L60 Hybrid SES (T2278)", "enum": ["T2278"] },
                { "title": "RoboVac L60 SES (T2277)", "enum": ["T2277"] },
                { "title": "RoboVac L70 Hybrid (T2190)", "enum": ["T2190"] },
                { "title": "RoboVac LR30 Hybrid (T2193)", "enum": ["T2193"] },
                { "title": "RoboVac LR30 Hybrid+ (T2181)", "enum": ["T2181"] },
                { "title": "RoboVac S1 Pro (T2080)", "enum": ["T2080"] },
                { "title": "RoboVac X8 (T2262)", "enum": ["T2262"] },
                { "title": "RoboVac X8 Hybrid (T2261)", "enum": ["T2261"] },
                { "title": "RoboVac X8 Pro SES (T2275)", "enum": ["T2275"] },
                { "title": "default", "enum": ["T0000"] }
              ]
            },
            "deviceId": {
              "title": "Device ID",
              "type": "string",
              "required": true
            },
            "localKey": {
              "title": "Local Key",
              "type": "string",
              "required": true
            },
            "deviceIp": {
              "title": "Device IP",
              "description": "If your device has a static IP, enter it here to improve performance.",
              "type": "string",
              "required": false
            },
            "useSwitchService": {
              "title": "Use Switch for Vacuum (Default is Fan)",
              "type": "boolean",
              "required": false,
              "default": false
            },
            "findButtonEnabled": {
              "title": "Enable Find Button",
              "type": "boolean",
              "required": false,
              "default": true
            },
            "batteryInformationEnabled": {
              "title": "Enable Battery and Charging Information",
              "type": "boolean",
              "required": false,
              "default": true
            },
            "errorSensorEnabled": {
              "title": "Enable Error Sensor",
              "type": "boolean",
              "required": false,
              "default": true
            }
          }
        }
      },
      "debugLogEnabled": {
        "title": "Enable Debug Log",
        "description": "Diverts the log messages for all log levels directly to the console",
        "type": "boolean",
        "required": false,
        "default": false
      }
    }
  }
}
